    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) Joindre",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/build/noon",
            "MIMode": "gdb"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/noon",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/local/bin/gdb"
        }
    ]
}